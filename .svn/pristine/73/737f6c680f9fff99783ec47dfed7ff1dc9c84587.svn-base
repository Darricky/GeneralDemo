<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:gravity="center"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/item_main_linear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingBottom="@dimen/marign_normal"
        android:paddingLeft="@dimen/marign_boundary"
        android:paddingRight="@dimen/marign_boundary"
        android:paddingTop="@dimen/marign_normal">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/marign_normal"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/item_reple_date_tip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="申请时间 : "
                android:textColor="@color/text_font_black_light"
                android:textSize="@dimen/size_small" />

            <TextView
                android:id="@+id/item_reple_date_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/item_reple_date_tip"
                android:text=""
                android:textColor="@color/text_font_black"
                android:textSize="@dimen/size_small" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:text="详情>>"
                android:textColor="@color/text_font_black"
                android:textSize="@dimen/size_mini" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/marign_normal"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="订单编号 : "
                android:textColor="@color/text_font_black_light"
                android:textSize="@dimen/size_small" />

            <TextView
                android:id="@+id/item_code_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/text_font_black"
                android:textSize="@dimen/size_small" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/marign_normal"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="下单时间 : "
                android:textColor="@color/text_font_black_light"
                android:textSize="@dimen/size_small" />

            <TextView
                android:id="@+id/item_date_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/text_font_black"
                android:textSize="@dimen/size_small" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/marign_normal"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="退款金额 : ¥"
                android:textColor="@color/orange_dark"
                android:textSize="@dimen/size_small" />

            <TextView
                android:id="@+id/item_money_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                android:textColor="@color/orange_dark"
                android:textSize="@dimen/size_small" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/marign_normal"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="退款原因 : "
                android:textColor="@color/text_font_black_light"
                android:textSize="@dimen/size_small" />

            <TextView
                android:id="@+id/item_reason_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/text_font_black"
                android:textSize="@dimen/size_small" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/item_container_rl"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginBottom="@dimen/marign_gridview"
            android:layout_marginTop="@dimen/marign_normal"
            android:orientation="horizontal">

            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@color/blue"
                android:visibility="invisible" />

            <HorizontalScrollView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_weight="4"
                android:scrollbars="none">

                <LinearLayout
                    android:id="@+id/item_container_linear"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"></LinearLayout>
            </HorizontalScrollView>
        </LinearLayout>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/divider_line_color" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/marign_middle"
        android:paddingTop="@dimen/marign_middle">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center|left">

                <TextView
                    android:id="@+id/item_button1_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/marign_boundary"
                    android:background="@drawable/shape_stroke_empty_solid_pink_right_angle"
                    android:paddingBottom="@dimen/marign_text_image"
                    android:paddingLeft="@dimen/marign_middle"
                    android:paddingRight="@dimen/marign_middle"
                    android:paddingTop="@dimen/marign_text_image"
                    android:text=" 同意 "
                    android:textColor="@color/white"
                    android:textSize="@dimen/size_middle" />

                <TextView
                    android:id="@+id/item_button2_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/marign_middle"
                    android:background="@drawable/shape_stroke_empty_solid_gray_right_angle"
                    android:paddingBottom="@dimen/marign_text_image"
                    android:paddingLeft="@dimen/marign_middle"
                    android:paddingRight="@dimen/marign_middle"
                    android:paddingTop="@dimen/marign_text_image"
                    android:text=" 拒绝 "
                    android:textColor="@color/white"
                    android:textSize="@dimen/size_middle" />
            </LinearLayout>

            <TextView
                android:id="@+id/item_status_tv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white"
                android:gravity="center|left"
                android:paddingLeft="@dimen/marign_boundary"
                android:text="状态:"
                android:textColor="@color/text_font_pink"
                android:textSize="@dimen/size_middle"
                android:visibility="invisible" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:gravity="center|right">

            <TextView
                android:id="@+id/item_name_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/marign_normal"
                android:layout_toLeftOf="@+id/item_mobile_linear"
                android:singleLine="true"
                android:text="客户:咔咔"
                android:textColor="@color/text_font_black"
                android:textSize="@dimen/size_small" />

            <LinearLayout
                android:id="@+id/item_mobile_linear"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/marign_middle"
                android:background="@drawable/shape_stroke_empty_solid_green"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/marign_text_image"
                android:paddingTop="@dimen/marign_text_image">

                <ImageView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:src="@drawable/icon_more_phone" />

                <TextView
                    android:id="@+id/item_phone_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="@dimen/marign_text_image"
                    android:paddingTop="@dimen/marign_text_image"
                    android:text="18205183644"
                    android:textColor="@color/white"
                    android:textSize="@dimen/size_small" />
            </LinearLayout>

            <!--<RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
