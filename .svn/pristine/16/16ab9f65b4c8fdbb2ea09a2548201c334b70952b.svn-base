<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <include
        android:id="@+id/top_title"
        layout="@layout/activity_base_top_bar"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="@string/activity_title_weight" />

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/divider_line_color" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="@string/activity_body_weight">

        <com.techfly.demo.selfview.FlexibileScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:id="@+id/base_linear"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="visible"
                    >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/marign_middle">

                        <ImageView
                            android:id="@+id/refund_info_iv"
                            android:layout_width="30dp"
                            android:layout_height="25dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/marign_boundary"
                            android:src="@drawable/icon_refund" />

                        <TextView
                            android:id="@+id/refund_info_tip"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBaseline="@+id/refund_info_iv"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/marign_middle"
                            android:layout_toRightOf="@id/refund_info_iv"
                            android:text="退款信息"
                            android:textColor="@color/text_font_pink"
                            android:textSize="@dimen/size_big" />

                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="@dimen/marign_normal"
                        android:background="@color/white">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginLeft="@dimen/marign_boundary"
                            android:background="@color/divider_line_color" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/marign_normal"
                        android:layout_marginLeft="@dimen/marign_bigger"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="退款原因 : "
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_middle" />

                        <TextView
                            android:id="@+id/detail_reason_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/marign_middle"
                            android:text=""
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_middle" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/item_container_rl"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginBottom="@dimen/margin_minier"
                        android:layout_marginTop="@dimen/marign_normal"
                        android:layout_marginLeft="@dimen/marign_boundary"
                        android:layout_marginRight="@dimen/marign_boundary"
                        android:orientation="horizontal">

                        <View
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            />

                        <HorizontalScrollView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_alignParentTop="true"
                            android:layout_weight="4"
                            android:scrollbars="none">
                            <LinearLayout
                                android:id="@+id/detail_container_linear"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal"></LinearLayout>
                        </HorizontalScrollView>
                    </LinearLayout>



                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/marign_normal"
                    android:background="@color/divider_line_blue">
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/marign_big">

                    <ImageView
                        android:id="@+id/order_info_iv"
                        android:layout_width="30dp"
                        android:layout_height="25dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/marign_boundary"
                        android:src="@drawable/icon_order_info" />

                    <TextView
                        android:id="@+id/order_info_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBaseline="@+id/order_info_iv"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/marign_middle"
                        android:layout_toRightOf="@id/order_info_iv"
                        android:text="订单信息"
                        android:textColor="@color/text_font_pink"
                        android:textSize="@dimen/size_big" />

                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/marign_normal"
                    android:background="@color/white">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="@dimen/marign_boundary"
                        android:background="@color/divider_line_color" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="visible">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/marign_bigger"
                        android:layout_marginTop="@dimen/marign_middle"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="订单编号: "
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_middle" />

                        <TextView
                            android:id="@+id/order_number_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_middle" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/marign_bigger"
                        android:layout_marginTop="@dimen/marign_middle"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="下单时间: "
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_middle" />

                        <TextView
                            android:id="@+id/order_time_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_middle" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/marign_middle"
                        android:layout_marginLeft="@dimen/marign_bigger"
                        android:layout_marginTop="@dimen/marign_middle"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="备注信息: "
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_middle" />

                        <TextView
                            android:id="@+id/order_remark_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_middle" />
                    </LinearLayout>
                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/marign_normal"
                    android:background="@color/divider_line_blue">

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/marign_middle"
                    android:visibility="gone">

                    <ImageView
                        android:id="@+id/logistics_info_iv"
                        android:layout_width="30dp"
                        android:layout_height="25dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/marign_boundary"
                        android:src="@drawable/icon_logistics_info" />

                    <TextView
                        android:id="@+id/logistics_info_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBaseline="@+id/logistics_info_iv"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/marign_middle"
                        android:layout_toRightOf="@id/logistics_info_iv"
                        android:text="物流信息"
                        android:textColor="@color/orange_dark"
                        android:textSize="@dimen/size_big" />

                    <TextView
                        android:id="@+id/logistics_status_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBaseline="@+id/logistics_info_iv"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/marign_larger"
                        android:text=""
                        android:textColor="@color/text_font_gray"
                        android:textSize="@dimen/size_big" />
                </RelativeLayout>

                <View
                    android:id="@+id/logistics_divider_view"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/marign_normal"
                    android:background="@color/divider_line_color"
                    android:visibility="gone" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:layout_marginRight="25dp"
                        android:background="@color/white"
                        android:orientation="horizontal"
                        android:paddingBottom="@dimen/marign_middle"
                        android:paddingTop="@dimen/marign_middle">

                        <TextView
                            android:id="@+id/order_status_tv1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:gravity="left"
                            android:text="商家已接单"
                            android:textColor="@color/text_font_black_light"
                            android:textSize="@dimen/size_smaller" />

                        <TextView
                            android:id="@+id/order_status_tv2"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:paddingRight="@dimen/marign_normal"
                            android:text="正在配货"
                            android:textColor="@color/text_font_black_light"
                            android:textSize="@dimen/size_smaller" />

                        <TextView
                            android:id="@+id/order_status_tv3"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:paddingLeft="@dimen/marign_normal"
                            android:text="配送中"
                            android:textColor="@color/text_font_black_light"
                            android:textSize="@dimen/size_smaller" />

                        <TextView
                            android:id="@+id/order_status_tv4"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:paddingLeft="@dimen/marign_bigger"
                            android:text="已收货"
                            android:textColor="@color/text_font_black_light"
                            android:textSize="@dimen/size_smaller" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/marign_bigger"
                        android:layout_marginLeft="@dimen/marign_large"
                        android:layout_marginRight="@dimen/marign_large"
                        android:background="@color/white"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/marign_boundary"
                        android:paddingRight="@dimen/marign_boundary">

                        <TextView
                            android:id="@+id/order_point_tv1"
                            android:layout_width="@dimen/marign_big"
                            android:layout_height="@dimen/marign_big"
                            android:layout_centerVertical="true"
                            android:background="@drawable/shape_stroke_circle_gray_point"
                            android:gravity="center"
                            android:textColor="@color/white"
                            android:textSize="@dimen/size_middle" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_weight="1"
                            android:background="@color/divider_line_color" />

                        <TextView
                            android:id="@+id/order_point_tv2"
                            android:layout_width="@dimen/marign_big"
                            android:layout_height="@dimen/marign_big"
                            android:layout_centerVertical="true"
                            android:background="@drawable/shape_stroke_circle_gray_point"
                            android:gravity="center"
                            android:textColor="@color/white"
                            android:textSize="@dimen/size_middle" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_weight="1"
                            android:background="@color/divider_line_color" />

                        <TextView
                            android:id="@+id/order_point_tv3"
                            android:layout_width="@dimen/marign_big"
                            android:layout_height="@dimen/marign_big"
                            android:layout_centerVertical="true"
                            android:background="@drawable/shape_stroke_circle_gray_point"
                            android:gravity="center"
                            android:textColor="@color/white"
                            android:textSize="@dimen/size_middle" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_weight="1"
                            android:background="@color/divider_line_color" />

                        <TextView
                            android:id="@+id/order_point_tv4"
                            android:layout_width="@dimen/marign_big"
                            android:layout_height="@dimen/marign_big"
                            android:layout_centerVertical="true"
                            android:background="@drawable/shape_stroke_circle_gray_point"
                            android:gravity="center"
                            android:textColor="@color/white"
                            android:textSize="@dimen/size_middle" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="visible">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/marign_middle">

                        <ImageView
                            android:id="@+id/address_info_iv"
                            android:layout_width="30dp"
                            android:layout_height="25dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/marign_boundary"
                            android:src="@drawable/icon_customer_info" />

                        <TextView
                            android:id="@+id/address_info1_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBaseline="@+id/address_info_iv"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/marign_middle"
                            android:layout_toRightOf="@id/address_info_iv"
                            android:text="客户信息"
                            android:textColor="@color/text_font_pink"
                            android:textSize="@dimen/size_big" />

                        <LinearLayout
                            android:id="@+id/detail_phone_linear"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="@dimen/marign_bigger"
                            android:background="@drawable/shape_stroke_empty_solid_green"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:paddingBottom="@dimen/marign_mini"
                            android:paddingTop="@dimen/marign_mini">

                            <ImageView
                                android:layout_width="18dp"
                                android:layout_height="18dp"
                                android:src="@drawable/icon_more_phone" />

                            <TextView
                                android:id="@+id/detail_phone_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="联系客户"
                                android:textColor="@color/white"
                                android:textSize="@dimen/size_middle" />
                        </LinearLayout>

                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="@dimen/marign_normal"
                        android:background="@color/white">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginLeft="@dimen/marign_boundary"
                            android:background="@color/divider_line_color" />
                    </LinearLayout>


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="@dimen/marign_middle"
                        android:paddingTop="@dimen/marign_normal">

                        <TextView
                            android:id="@+id/address_name_tip_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_marginLeft="@dimen/marign_bigger"
                            android:text="姓名: "
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_smaller" />

                        <TextView
                            android:id="@+id/address_name_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@+id/address_name_tip_tv"
                            android:text=""
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_smaller" />

                        <TextView
                            android:id="@+id/address_phone_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="@dimen/marign_bigger"
                            android:text=""
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_smaller" />

                        <TextView
                            android:id="@+id/address_level_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/address_name_tip_tv"
                            android:layout_below="@+id/address_name_tip_tv"
                            android:layout_marginTop="@dimen/marign_normal"
                            android:text="客户身份:会员"
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_smaller" />

                        <TextView
                            android:id="@+id/address_detail_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/address_name_tip_tv"
                            android:layout_below="@+id/address_level_tv"
                            android:layout_marginTop="@dimen/marign_normal"
                            android:text="收货地址: "
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_smaller" />
                    </RelativeLayout>
                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/marign_normal"
                    android:background="@color/divider_line_blue">
                    <!--<View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentTop="true"
                        android:background="@color/divider_line_color"
                        />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentBottom="true"
                        android:background="@color/divider_line_color"
                        />-->
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/marign_middle">

                        <ImageView
                            android:id="@+id/goods_info_iv"
                            android:layout_width="30dp"
                            android:layout_height="25dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/marign_boundary"
                            android:src="@drawable/icon_goods_info" />

                        <TextView
                            android:id="@+id/goods_info_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBaseline="@+id/goods_info_iv"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/marign_middle"
                            android:layout_toRightOf="@id/goods_info_iv"
                            android:text="商品信息"
                            android:textColor="@color/text_font_pink"
                            android:textSize="@dimen/size_big" />

                        <TextView
                            android:id="@+id/goods_status_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBaseline="@+id/goods_info_iv"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/marign_larger"
                            android:text=""
                            android:textColor="@color/text_font_gray"
                            android:textSize="@dimen/size_big" />
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="@dimen/marign_normal"
                        android:background="@color/white">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginLeft="@dimen/marign_boundary"
                            android:background="@color/divider_line_color" />
                    </LinearLayout>


                    <com.techfly.demo.selfview.ForbidScrollListView
                        android:id="@+id/goods_lv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/marign_normal"
                        android:layout_marginRight="@dimen/marign_normal"
                        android:divider="@color/divider_line_color"
                        android:dividerHeight="0.5dp"></com.techfly.demo.selfview.ForbidScrollListView>

                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/marign_normal"
                    android:layout_marginTop="@dimen/marign_middle"
                    android:background="@color/divider_line_blue">
                    <!--<View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentTop="true"
                        android:background="@color/divider_line_color"
                        />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentBottom="true"
                        android:background="@color/divider_line_color"
                        />-->
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/marign_middle">

                        <ImageView
                            android:id="@+id/pay_info_iv"
                            android:layout_width="30dp"
                            android:layout_height="25dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/marign_boundary"
                            android:src="@drawable/icon_pay_info" />

                        <TextView
                            android:id="@+id/pay_info_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBaseline="@+id/pay_info_iv"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/marign_middle"
                            android:layout_toRightOf="@id/pay_info_iv"
                            android:text="支付信息"
                            android:textColor="@color/text_font_pink"
                            android:textSize="@dimen/size_big" />
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="@dimen/marign_normal"
                        android:background="@color/white">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginLeft="@dimen/marign_boundary"
                            android:background="@color/divider_line_color" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/marign_normal"
                        android:layout_marginTop="@dimen/marign_normal"
                        android:orientation="horizontal"
                        android:visibility="visible">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="支付方式"
                            android:textColor="@color/text_font_pink"
                            android:textSize="@dimen/size_smaller" />

                        <TextView
                            android:id="@+id/pay_type_tv"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text=""
                            android:textColor="@color/orange_dark"
                            android:textSize="@dimen/size_smaller" />
                    </LinearLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="@dimen/marign_middle"
                        android:paddingTop="@dimen/marign_normal">

                        <TextView
                            android:id="@+id/pay_info_tv1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_marginLeft="@dimen/marign_bigger"
                            android:text="订单:¥0.00"
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_smaller" />

                        <TextView
                            android:id="@+id/pay_info_tv2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/pay_info_tv1"
                            android:layout_below="@+id/pay_info_tv1"
                            android:layout_marginTop="@dimen/marign_normal"
                            android:text="实付款: ¥0.00"
                            android:textColor="@color/text_font_black"
                            android:textSize="@dimen/size_smaller" />
                    </RelativeLayout>


                </LinearLayout>


                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginTop="@dimen/marign_bigger"
                    android:background="@color/divider_line_color"
                    />

                <LinearLayout
                    android:id="@+id/detail_bottom_linear"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/marign_bigger"
                    android:gravity="center"
                    android:visibility="gone"
                    >

                    <TextView
                        android:id="@+id/detail_confirm_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/marign_boundary"
                        android:background="@drawable/shape_stroke_empty_solid_pink_right_angle"
                        android:paddingBottom="@dimen/marign_mini"
                        android:paddingLeft="@dimen/marign_middle"
                        android:paddingRight="@dimen/marign_middle"
                        android:paddingTop="@dimen/marign_mini"
                        android:text="同意申请"
                        android:textColor="@color/white"
                        android:textSize="@dimen/size_big" />

                    <TextView
                        android:id="@+id/detail_refuse_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/marign_larger"
                        android:background="@drawable/shape_stroke_empty_solid_gray_right_angle"
                        android:paddingBottom="@dimen/marign_mini"
                        android:paddingLeft="@dimen/marign_middle"
                        android:paddingRight="@dimen/marign_middle"
                        android:paddingTop="@dimen/marign_mini"
                        android:text="拒绝申请"
                        android:textColor="@color/white"
                        android:textSize="@dimen/size_big" />
                </LinearLayout>

            </LinearLayout>
        </com.techfly.demo.selfview.FlexibileScrollView>
    </LinearLayout>

</LinearLayout>