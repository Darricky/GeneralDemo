<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/marign_normal"
        android:paddingBottom="@dimen/marign_normal"
        >

        <TextView
            android:id="@+id/item_date_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="@dimen/marign_boundary"
            android:text="0000.00.00"
            android:textColor="@color/text_font_black"
            android:textSize="@dimen/size_middle"
            />

        <RatingBar
            android:id="@+id/item_ratingbar"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/marign_middle"
            android:progressDrawable="@drawable/rating_yellow"
            style="?android:attr/ratingBarStyleIndicator"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/marign_boundary"
            android:layout_centerVertical="true"
            android:numStars="5"
            android:stepSize="1"
            android:rating="5"
            />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/divider_line_color" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/marign_boundary"
            android:layout_marginRight="@dimen/marign_boundary"
            android:paddingTop="@dimen/marign_mini"
            android:paddingBottom="@dimen/marign_mini"
            android:orientation="horizontal"
            android:gravity="center"
            >
            <TextView
                android:id="@+id/item_name_tv"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right|top"
                android:textSize="@dimen/size_middle"
                android:textColor="@color/text_font_black_light"
                android:text=""
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center|top"
                android:textSize="@dimen/size_middle"
                android:textColor="@color/text_font_black_light"
                android:text=" : "
                />
            <TextView
                android:id="@+id/item_phone_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:layout_marginRight="@dimen/marign_middle"
                android:gravity="left"
                android:textSize="@dimen/size_middle"
                android:textColor="@color/text_font_black_light"
                android:text=""
                />
        </LinearLayout>

        <TextView
            android:id="@+id/item_reply_status_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_stroke_empty_solid_red"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/marign_boundary"
            android:textSize="@dimen/size_small"
            android:textColor="@color/white"
            android:text="已回复"
            android:visibility="invisible"
            />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <!--
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/marign_boundary"
                    android:layout_marginRight="@dimen/marign_boundary"
                    android:paddingTop="@dimen/marign_normal"
                    android:paddingBottom="@dimen/marign_text_image"
                    android:orientation="horizontal"
                    android:gravity="center"
                    >
                    <TextView
                        android:id="@+id/item_name_tv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:textSize="@dimen/size_middle"
                        android:textColor="@color/text_font_black_light"
                        android:text=""
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|top"
                        android:textSize="@dimen/size_middle"
                        android:textColor="@color/text_font_black_light"
                        android:text=" : "
                        />
                    <TextView
                        android:id="@+id/item_phone_tv"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="4"
                        android:gravity="left|top"
                        android:textSize="@dimen/size_middle"
                        android:textColor="@color/text_font_black_light"
                        android:text=""
                        />

                </LinearLayout> -->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/marign_boundary"
            android:layout_marginRight="@dimen/marign_boundary"
            android:paddingTop="@dimen/marign_mini"
            android:paddingBottom="@dimen/marign_mini"
            android:orientation="horizontal"
            android:gravity="center"
            >
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="right"
                android:textSize="@dimen/size_middle"
                android:textColor="@color/text_font_black_light"
                android:text="订单号"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/size_middle"
                android:textColor="@color/text_font_black_light"
                android:text=" : "
                />
            <TextView
                android:id="@+id/item_order_number_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:gravity="left"
                android:textSize="@dimen/size_middle"
                android:textColor="@color/text_font_black_light"
                android:text=""
                />

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/marign_boundary"
                android:layout_marginRight="@dimen/marign_boundary"
                android:paddingTop="@dimen/marign_mini"
                android:paddingBottom="@dimen/marign_mini"
                android:orientation="horizontal"
                android:gravity="center"
                >
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="right|top"
                    android:textSize="@dimen/size_middle"
                    android:textColor="@color/text_font_black_light"
                    android:text="评论"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center|top"
                    android:textSize="@dimen/size_middle"
                    android:textColor="@color/text_font_black_light"
                    android:text=" : "
                    />
                <TextView
                    android:id="@+id/item_comment_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:layout_marginRight="@dimen/marign_middle"
                    android:gravity="left"
                    android:textSize="@dimen/size_middle"
                    android:textColor="@color/text_font_black_light"
                    android:text=""
                    />
            </LinearLayout>

            <ImageView
                android:id="@+id/item_delete_iv"
                android:layout_width="22dp"
                android:layout_height="25dp"
                android:layout_alignParentTop="true"
                android:layout_marginRight="@dimen/marign_middle"
                android:padding="@dimen/margin_minier"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:src="@drawable/icon_clear"
                android:visibility="invisible"
                />

        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/item_container_rl"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="@dimen/marign_mini"
        android:layout_marginBottom="@dimen/margin_minier"
        android:layout_marginLeft="@dimen/marign_boundary"
        android:layout_marginRight="@dimen/marign_boundary"
        android:orientation="horizontal"
        >
        <View
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@color/blue"
            android:visibility="invisible"
            />
        <HorizontalScrollView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:layout_alignParentTop="true"
            android:scrollbars="none"
            >
            <LinearLayout
                android:id="@+id/item_container_linear"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                >
            </LinearLayout>
        </HorizontalScrollView>
    </LinearLayout>

    <View
        android:id="@+id/iaddm_line"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/divider_line_color" />

    <LinearLayout
        android:id="@+id/address_linear2"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_below="@id/iaddm_line"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/item_contact_tv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:padding="@dimen/marign_normal"
            android:text="联系客户"
            android:textColor="@color/orange_dark"
            android:textSize="@dimen/size_middle" />

        <TextView
            android:layout_width="0.5dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:background="@color/divider_line_color"
            android:gravity="center"
            />

        <TextView
            android:id="@+id/item_reply_tv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:padding="@dimen/marign_normal"
            android:text="回复评论"
            android:textColor="@color/orange_dark"
            android:textSize="@dimen/size_middle" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/divider_line_color" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/marign_mid"
        android:background="@color/main_gray"
        />

</LinearLayout>